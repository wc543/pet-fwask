{
  "name": "back",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "scripts": {
    "setup": "rm -f database.db && sqlite3 database.db < setup.sql",
    "build": "rm -rf out && mkdir out && tsc",
    "start": "cd out && node server.js",
    "dev": "ts-node src/server.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --rootDir=src",
    "test-data": "sqlite3 database.db < test-data.sql",
    "watch": "npm run build && tsc-watch --onSuccess \"npm run start\"",
    "start-pm2": "PORT=3001 pm2 start npm --name 'Pet-FWASK' -- run start",
    "delete-pm2": "pm2 delete Pet-FWASK",
    "restart-pm2": "pm2 restart Pet-FWASK",
    "logs-pm2": "pm2 logs Pet-FWASK"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "back": "file:",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "nodemon": "^3.1.9",
    "socket.io": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.2"
  },
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.5",
    "@types/node": "^22.13.4",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.5.14",
    "axios": "^1.6.5",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.3"
  }
}
